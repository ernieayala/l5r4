  "SETTINGS": {
    "showTraitRollOptions": {
      "name": "Ask for Trait Rolls options by default.",
      "label": "Hold Shift to Toggle."
    },
    "showSpellRollOptions": {
      "name": "Ask if the Ring Roll is a spell by default.",
      "label": "Hold Shift to Toggle."
    },
    "showSkillRollOptions": {
      "name": "Ask for Skill Rolls and Damage Roll options by default.",
      "label": "Hold Shift to Toggle."
    },
    "allowNpcVoidPoints": {
      "name": "Display Void Point checkbox for NPC rolls.",
      "label": "NPCs normally lack Void."
    },
    "useLtTenDiceRule": {
      "name": "Use the modified Ten Dice Rule from Little Truths.",
      "label": "If there are an odd number of excess rolled dice and fewer than 10 kept dice, add 2 to the roll total so the \"extra\" die gives a benefit."
    },
    "usePcTabs": {
      "name": "Alternative layout for the pc sheet",
      "label": "Use an alternative sheet with sections divided in tabs"
    },
    "calculateRank": {
      "name": "Calculate Insight Rank automatically",
      "label": "Insight Rank is updated automatically based on total Insight Points"
    },
    "allowArmorStacking": {
      "name": "Allow armor stacking",
      "label": "Multiple pieces of \"armor\" can be stacked (to represent buffs, etc.)"
    },
    "showWeaponRollOptions": {
      "name": "Ask for Weapon Roll options by default.",
      "label": "Hold Shift to Toggle."
    },
    "LtException": {
      "name": "Apply Little Truths exception",
      "label": "When dice keep is reduced below 10 by conversion, add 2."
    },
    "runMigration": {
      "name": "Run migrations on startup",
      "label": "If enabled, pending data migrations will run the next time the world loads. Disable to skip migrations for troubleshooting."
    },
